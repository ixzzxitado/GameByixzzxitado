sleep 1
echo ' '
echo -e " \033[1;32m [!]\033[0m \033[1;33m Yes or No (y/n) \033[0m"   
echo ' '
echo ' '
echo ' '
read -p ">>>>>>>>>>  " yn

case $yn in
        [yY] ) echo -e "\033[1;31m why u dont press no?\033[0m" ;

sleep 2

clear

sleep 2

bash oppositor

                break;;
       [nN] ) echo -e "\033[1;31m DONT EXIST CHANCE \033[0m" ;

sleep 2

clear

sleep 1

echo -e "\033[1;31m D0NT RUN.\033[0m"

sleep 2

bash oppositor

                break;;
           [_] ) echo '' ;
                exit;;
        * ) echo -e " \033[1;33m [!]\033[0m \033[5;30m Wrong Option.\033[0m"
esac

sleep 1

clear

done
